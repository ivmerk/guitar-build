    "mock:server": "json-server ./mocks/mock-server-data.json --port 3123"
  },
  "devDependencies": {
    "@types/convict": "6.1.1",
    "@types/convict-format-with-validator": "6.0.2",
    "@types/multer": "1.4.7",
    "@types/node": "18.11.13",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "eslint": "8.29.0",
    "eslint-config-htmlacademy": "8.0.0",
    "nodemon": "2.0.20",
    "json-server": "0.17.1",
    "pino-pretty": "9.1.1",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "type": "module",
  "engines": {
    "node": "18.15.0",
    "npm": "9.6.4"
  },
