    "dotenv": "16.0.3",
    "got": "12.5.3",
    "http-status-codes": "2.2.0",
    "jose": "4.11.1",
    "mime-types": "2.1.35",
    "mongoose": "6.7.2",
    "multer": "1.4.5-lts.1",
    "nanoid": "4.0.0",
    "pino": "8.7.0",
    "reflect-metadata": "0.1.13"
